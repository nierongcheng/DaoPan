
.my-top-container {
	width: 100%;
	padding-top: 64rpx;
	height: 530rpx;
}
.my-score-container {
	width: 204rpx;
	margin: 36rpx auto 0;
	background-image: url(data:image/png;base64,);
	background-size: 100% auto;
	background-repeat: no-repeat;
}
.my-score-container::before {
	content: "";
	padding-top: 100%;
	display: block;
}
.my-score {
	font-size: 74rpx;
}
.my-bottom-container {
	width: 100%;
	position: relative;
	margin-top: -100rpx;
	overflow: hidden;
}
.my-bottom-container::before {
	content: '';
	width: 120%;
	height: 300rpx;
	position: absolute;
	left: -10%;
	top: 0;
	-webkit-border-radius: 50% 50% 0 0;
	        border-radius: 50% 50% 0 0;
	background-color: #f1f1f1;
}
.my-works-img {
	width: 200rpx;
	height: 266rpx;
	margin-top: 60rpx;
	margin-bottom: 12rpx;
}


