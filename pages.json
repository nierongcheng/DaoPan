{
	"easycom": {
		"autoscan": true,
		"custom": {
			"^u-(.*)": "@/uview-ui/components/u-$1/u-$1.vue"
		}
	},
	"pages": [
		//pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
        {
            "path" : "pages/dimension/dimension",
            "style" : {
				"navigationStyle":"custom",
				"app-plus":{
					"titleNView":false
				}
			}
        }
        ,{
            "path" : "pages/history/history",
            "style" : {
				"navigationBarTitleText": "已评测列表"
			}
        }
        ,{
            "path" : "pages/mine/mine",
            "style" : {
				"navigationBarTitleText": "我的"
			}
        }
        ,{
            "path" : "pages/login/login/login",
            "style" : {}
        }
        ,{
            "path" : "pages/dimensionDetail/dimensionDetail",
            "style" : {
				"navigationBarTextStyle":"white",
				"navigationBarBackgroundColor":"#29AD67"
			}
        }
        ,{
            "path" : "pages/common/webview/webview",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "详情",
                "enablePullDownRefresh": false
            }
            
        }
        ,{
            "path" : "pages/scoreOverview/scoreOverview",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "得分概览",
                "enablePullDownRefresh": false
            }
            
        }
        ,{
            "path" : "pages/upload/upload/upload",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "",
                "enablePullDownRefresh": false
            }
            
        }
        ,{
            "path" : "pages/artist/applyArtist/applyArtist",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "",
                "enablePullDownRefresh": false
            }
            
        }
    ],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F0AD4E"
	},
	"tabBar": {
		"borderStyle": "black",
		"backgroundColor": "#333",
		"color": "#8F8F94",
		"selectedColor": "#f33e54",
		"list": [{
				"pagePath": "pages/dimension/dimension",
				"iconPath": "static/img/tabbar/home.png",
				"selectedIconPath": "static/img/tabbar/homeactive.png",
				"text": "量化"
			},
			{
				"pagePath": "pages/history/history",
				"iconPath": "static/img/tabbar/guanzhu.png",
				"selectedIconPath": "static/img/tabbar/guanzhuactive.png",
				"text": "历史"
			},
			{
				"pagePath": "pages/mine/mine",
				"iconPath": "static/img/tabbar/me.png",
				"selectedIconPath": "static/img/tabbar/meactive.png",
				"text": "我的"
			}
		]
	}
}
